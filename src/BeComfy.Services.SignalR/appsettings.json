{
  "app": {
    "name": "signalr-service"
  },
  "logger": {
    "enabled": true,
    "writeToConsole": true,
    "minimumLevel": "Information",
    "logStorage": "ElasticSearch",
    "loggerConfig": {
        "indexPattern": "becomfy-{yyyy-MM-dd}",
        "loggerConnectionOptions": {
            "scheme": "http",
            "url": "localhost",
            "port": 9200
        }
    }
  },
  "rabbitMq": {
    "namespace": "signalr",
    "retries": 3,
    "retryInterval": 2,
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost"
    ],
    "requestTimeout": "00:00:10",
    "publishConfirmTimeout": "00:00:01",
    "recoveryInterval": "00:00:10",
    "persistentDeliveryMode": true,
    "autoCloseConnection": true,
    "automaticRecovery": true,
    "topologyRecovery": true,
    "exchange": {
      "durable": true,
      "autoDelete": false,
      "type": "Topic"
    },
    "queue": {
      "autoDelete": false,
      "durable": true,
      "exclusive": false
    }
  },
  "jwt": {
    "secretKey": "aUJsAgwCSPCuYYgnGSVBWUgXLUZtNqPXTDPsvKKffYZMXMEuzpaYHBvYdWfHxcptFUUQXH",
    "expiryMinutes": 30,
    "issuer": "becomfy-identity-service",
    "validateLifetime": true
  }
}
